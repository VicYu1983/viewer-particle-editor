<dialog width="500" title="儲存至雲端">
    <tableview id="tv_particleEffectList" width="100%" height="200" contentWidth="100%">
        <header width="500">
            <!-- <column id="pid" text="id" width="30" /> -->
            <column id="name" text="名稱" width="100" />
            <column id="describtion" text="描述" width="100%" />
        </header>
        <data>
            <!-- <item pid="0" name="平面" describtion="平面" /> -->
        </data>
    </tableview>
    <textfield id="txt_effectName" text="" width="100%" placeholder="請輸入特效名稱" />
    <textarea id="txt_effectDescribtion" text="" width="100%" placeholder="請輸入特效描述" />
    <hbox width="100%">
        <button id="btn_delete" width="100%" value="刪除"></button>
        <button id="btn_load" width="100%" value="讀取"></button>
        <button id="btn_save" width="100%" value="儲存"></button>
        <!-- <button id="btn_saveAs" width="100%" value="另存新檔"></button> -->
        <button id="btn_close" width="100%" value="關閉"></button>
